_FORMULARIO:\n            return {\n                ...state,\n                formulario: true\n            }\n\n        case OBTENER_PROYECTOS:\n            return {\n                ...state,\n                proyectos: action.payload\n            }\n\n        case AGREGAR_PROYECTOS:\n            return{\n                ...state,\n                proyectos: [...state.proyectos, action.payload]\n            }\n    }\n}","/Users/mac/ironhack/lab/portfolio/portfolio-client/src/types/index.js",[],"/Users/mac/ironhack/lab/portfolio/portfolio-client/src/components/Proyectos/ListadoProyectos.js",["169"],"import React, { useEffect, useContext } from 'react';\n\nimport proyectoContext from '../../context/proyectos/proyectoContext';\nimport Proyecto from '../Proyectos/Proyecto';\n\nexport default function ListadoProyectos() {\n\n    const proyectosContext = useContext(proyectoContext);\n    const { proyectos, obtenerProyectos } = proyectosContext;\n\n    useEffect(() => {\n        obtenerProyectos();\n    }, [])\n\n    if(!proyectos) return null;\n\n\n    return (\n        <ul>\n            {proyectos.map(proyecto => (\n                <Proyecto\n                    key= {proyecto.id}\n                    proyecto= {proyecto}\n                />\n            ))}\n        </ul>\n    )\n}\n","/Users/mac/ironhack/lab/portfolio/portfolio-client/src/components/Proyectos/Proyecto.js",[],"/Users/mac/ironhack/lab/portfolio/portfolio-client/src/context/alertas/alertaState.js",[],"/Users/mac/ironhack/lab/portfolio/portfolio-client/src/context/alertas/alertaContext.js",[],"/Users/mac/ironhack/lab/portfolio/portfolio-client/src/context/alertas/alertaReducer.js",["170"],"import {\n    MOSTRAR_ALERTA,\n    OCULTAR_ALERTA\n} from '../../types';\n\n\nexport default (state, action) => {\n    switch(action.type) {\n\n            case MOSTRAR_ALERTA:\n                return {\n                    alerta: action.payload\n                }\n            case OCULTAR_ALERTA:\n                re