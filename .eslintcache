s=\"fa fa-envelope-o fa-fw\"></i>\n                            <input\n                                className=\"log-form-group\"\n                                type= 'text'\n                                placeholder='email'\n                                name= 'email'\n                                value= {email}\n                                onChange={onChange}\n                            />\n\n                        </div>\n                    \n\n                \n                            \n                        <div className=\"log-form-group\">\n\n                            <input\n                                className=\"log-form-group\"\n                                type= 'password'\n                                placeholder='password'\n                                name= 'password'\n                                value= {password}\n                                onChange={onChange}\n                            />\n\n                        </div>\n                \n                    \n                    <div className=\"log-form-group\">\n                        <input \n                            className=\"button\"\n                            type='submit'\n                            value='Login'\n                        />\n                    </div>\n                \n                </div>\n                \n            </form>\n\n        </div>\n    </div>\n\n        \n    )\n}\n","/Users/mac/ironhack/lab/portfolio/portfolio-client/src/config/token.js",[],"/Users/mac/ironhack/lab/portfolio/portfolio-client/src/components/Proyectos/ProyectosScreen.js",[],"/Users/mac/ironhack/lab/portfolio/portfolio-client/src/components/Layout/Main.js",["151"],"import React, { useContext, useEffect } from 'react';\n\nimport Sidebar from './Sidebar';\nimport Topbar from './Topbar';\nimport Rightbar from './Rightbar';\n\nimport './Main.css';\n\nimport AuthContext from '../../context/auth/authContext';\n\n\nexport default function 