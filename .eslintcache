  }\n\n        iniciarSesion({\n            email, \n            password\n        })\n\n        guardarUsuario({\n            email:'',\n            password: ''\n        })\n\n        // console.log(usuario)\n    }\n\n\n    return (\n\n    <div className=\"register-background\">\n        <div className=\"register-container\">\n\n        {alerta ? (<div className={`${alerta.categoria}`}>{alerta.msg}</div>) : null}\n\n            <form\n                onSubmit = { onSubmit }\n            >   \n                \n                    <div className=\"login-wrap\">\n                        <div className=\"log-form-group\">\n                        \n                            <i class=\"fa fa-envelope-o fa-fw\"></i>\n                            <input\n                                className=\"log-form-group\"\n                                type= 'text'\n                                placeholder='email'\n                                name= 'email'\n                                value= {email}\n                                onChange={onChange}\n                            />\n\n                        </div>\n                    \n\n                \n                            \n                        <div className=\"log-form-group\">\n\n                            <input\n                                className=\"log-form-group\"\n                                type= 'password'\n                                placeholder='password'\n                                name= 'password'\n                                value= {password}\n                                onChange={onChange}\n                            />\n\n                        </div>\n                \n                    \n                    <div className=\"log-form-group\">\n                        <input \n                            className=\"button\"\n                            type='submit'\n                            value='Login'\n                        />\n                    </div>\n                \n                </div>\n                \n            </form>\n\n        </div>\n    </div>\n\n        \n    )\n}\n","/Users/mac/ironhack/lab/portfolio/portfolio-client/src/config/token.js",[],"/Users/mac/ironhack/lab/portfolio/portfolio-client/src/components/Proyectos/ProyectosScreen.js",[],"/Users/mac/ironhack/lab/portfolio/portfolio-client/src/components/Layout/Main.js",["161"],"import React, { useContext, useEffect } from 'react';\n\nimport Sidebar from './Sidebar';\nimport Topbar from './Topbar';\nimport Rightbar from './Rightbar';\n\nimport './Main.css';\n\nimport AuthContext from '../../context/auth/authContext';\n\n\nexport default function Main() {\n    \n    const authContext = useContext(AuthContext);\n    const { usuarioAutenticado } = authContext;\n\n    useEffect(() => {\n        usuarioAutenticado();\n    }, [])\n\n  \n    return (\n        <div className=\"main-container\">\n\n                <div className=\"left-area\">\n\n                    <Sidebar />\n\n                </div>\n\n                <div className=\"middle-area\">\n\n                    <Topbar />\n                    \n                </div>\n\n                <div className=\"right-area\">\n\n                    <Rightbar />\n                    \n                </div>\n\n        </div>\n    )\n}\n","/Users/mac/ironhack/lab/portfolio/portfolio-client/src/components/Proyectos/ProyectoNuevo.js",[],"/Users/mac/ironhack/lab/portfolio/portfolio-client/src/components/Layout/Sidebar.js",[],"/Users/mac/ironhack/lab/portfolio/portfolio-client/src/components/Layout/Rightbar.js",[],"/Users/mac/ironhack/lab/portfolio/portfolio-client/src/components/Layout/Topbar.js",["162"],"import React, { Fragment } from 'react';\nimport PrivateMenu from '../Global/PrivateMenu';\n\nimport './Topbar.css';\n\nexport default function Topbar() {\n    return (\n\n        <div className=\"topbar-container\">\n                 \n                <PrivateMenu />\n\n        </div>\n