\n\n    return (\n        \n        <div className=\"proyect-form-container\">\n\n        \n            <div className=\"proyect-form\">\n            \n            <button onClick={onClickFormulario}>Ver Fomulario</button>\n\n\n            { formulario ?\n\n                (\n                    <form\n                    onSubmit={onSubmit}\n                    encType='multipart/form-data'\n                >\n\n                    {error ? 'Completa todos los campos' : null}\n\n                    <div className=\"cajetin-form\">\n\n                        <input\n                            className=\"line-form\"\n                            type=\"text\"\n                            name='name'\n                            placeholder='nombre'\n                            value={name}\n                            onChange={onChange}\n                        />\n\n                    </div>\n\n                    <div className=\"cajetin-form\">\n                        \n                        <input\n                            className=\"line-form\"\n                            type=\"file\"\n                            name='productPicture'\n                            id='productPicture'\n                            placeholder='imagen'\n                            onChange={onChange}\n                        />\n\n                    </div>\n\n                    <div className=\"cajetin-form\">\n\n                        <select \n                            className=\"line-form\"\n                            name='category' \n                            onChange={onChange}\n                        >\n\n                        <option>-- Select --</option>\n                        {categorias.map((categorySelect) => (\n                            <option\n                                key={categorySelect.id}\n                                value={categorySelect.category}\n                            >\n                                {categorySelect